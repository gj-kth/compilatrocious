class TestJVM {
	public static void main(String[] args) {
		
		int a;
		Baaa obj;

		obj = new Baaa();
		a = obj.func(5,12, true);
		a = obj.func(17,18, false);
		System.out.println(a);
	}
}

class Baaa {
	public int func(int x, int y, boolean b){
		if(b){
			System.out.println(x);	
		}else{
			System.out.println(y);	
		}
		return 100;
	}
}
