class main {
    public static void main(String[] args) {
    	A a;
        C c;
        B b;
        a = new A();
        b = new B();
        c = new C();
        System.out.println(a.f());
        System.out.println(c.f());
        System.out.println(c.i());
        System.out.println(b.g());
        System.out.println(b.h());
    }
}

class A{
	public int f(){
		return 1;
	}

	public int g(){
		return 2;
	}
}

class B extends A{
	public int h(){
		return 3;
	}
}

class C extends A{
	public int i(){
		return 4;
	}
}